package Strings;

public class CharAtIndex {
	public static void main(String args[]) {
		String str="HelloJava";
		System.out.println(str);
		int index1=str.charAt(5);
		int index2=str.charAt(7);
		System.out.println("Char at index of 5 is"+(char)index1);
		System.out.println("Char at index of 5 is"+(char)index2);
		
	}

}
