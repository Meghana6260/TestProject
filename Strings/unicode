
package Strings;

public class UnicodeCodePoint {

	public static void main(String[] args) {
				String str="HelloJava";
				int val1=str.codePointBefore(4);
				int val2=str.codePointBefore(2);
				System.out.println("The value at index 4:"+val1);
				System.out.println("The value at index 2:"+val2);
				
				
			}

		}


	
