import java.io.*;
import java.math.BigInteger;
import java.util.*;

class UserMainCode {
    public String addNumberStrings(String input1, String input2) {
       
        BigInteger n1 = new BigInteger(input1);
        BigInteger n2 = new BigInteger(input2);

       
        BigInteger sum = n1.add(n2);

       
        return sum.toString();
    }
}
