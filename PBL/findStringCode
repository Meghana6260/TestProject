import java.io.*;
import java.util.*;

class UserMainCode {
    public int findStringCode(String input1) {
        String[] words = input1.split(" ");
        StringBuilder result = new StringBuilder();

        for (String word : words) {
            word = word.toUpperCase(); // case-insensitive
            int len = word.length();
            int sum = 0;

            for (int i = 0; i < len / 2; i++) {
                int firstVal = word.charAt(i) - 'A' + 1;
                int lastVal = word.charAt(len - 1 - i) - 'A' + 1;
                sum += Math.abs(firstVal - lastVal);
            }

            // Middle character if odd length
            if (len % 2 != 0) {
                int midVal = word.charAt(len / 2) - 'A' + 1;
                sum += midVal;
            }

            result.append(sum);
        }

        return Integer.parseInt(result.toString());
    }
}
