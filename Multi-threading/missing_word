package pk3;
import java.util.*;
public class missing_word {
	public static String identify(String input1,String input2) {
		input1=input1.trim();
		String[] candidate=input2.split(":");
		ArrayList<String>result = new ArrayList<String>();
		for(String word:candidate) {
			word=word.trim();
			if(word.length()!=input1.length()) continue;
			boolean match=true;
			for(int i=0;i<word.length();i++) {
				char c2=word.charAt(i);
				char c1=input1.charAt(i);
				
				if(c1=='_') continue;

                if (Character.toLowerCase(c1) != Character.toLowerCase(c2)) {
                    match = false;
                    break;
		}
	}

            if (match) {
                result.add(word.toUpperCase());
		}
	}
		return result.isEmpty() ? "ERROR-009" : String.join(":", result);
	}
	public static void main(String[] args) {
		String input1 = "Fi_er";
        String input2 = "Fever:filer:Filter: Fixer:fiber:fibre:tailor:offer";

        String output = identify(input1, input2);
        System.out.println("Output: " + output);
		
	

	}

}
